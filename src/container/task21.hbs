<div>
  <section>
    <div class='card'>
      <br />
      <div class='card-body'>
        {{#with heading}}
          <div class='card'>
            {{#with main}}
              <div class='card-body'>
                <h1 class='card-title'>{{title}}</h1>
                <p class='card-text'> {{text}}.</p>
                {{#with button_text}}
                  <button
                    class='btn btn-primary'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-primary'
                  >{{outline}}</button>
                {{/with}}
              </div>
            {{/with}}
          </div>
          <br />
          <div class='card'>
            {{#with second}}
              <div class='card-body'>
                <h2 class='card-title'>{{title}}</h2>
                <p class='card-text'> {{text}}.</p>
                {{#with button_text}}
                  <button
                    class='btn btn-secondary'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-secondary'
                  >{{outline}}</button>
                {{/with}}
              </div>
            {{/with}}
          </div>
          <br />
          <div class='card'>
            {{#with subblock}}
              <div class='card-body'>
                <h3 class='card-title'>{{title}}</h3>
                <p class='card-text'> {{text}}.</p>
                {{#with button_text}}
                  <button
                    class='btn btn-dark'
                  >{{solid}}</button>
                  <br />
                  <br />
                  <button
                    type='button'
                    class='btn btn-outline-dark'
                  >{{outline}}</button>
                {{/with}}
              </div>
            {{/with}}
          </div>
        {{/with}}
      </div>
    </div>
  </section>
  <section>
    <div class='card'>
      <div class='card-body'>
        {{#with nested_display}}
          <div class='display-1'>
            {{title}}
          </div>
          <br />
          <div class='card mb-3'>
            {{#with children_display}}
              <div class='card-body'>
                <div class='display-4'>{{title}}</div>
                <p class='card-text'>{{text}}</p>
                {{#with button}}
                  <button
                    type='button'
                    class='btn btn-warning'
                  >
                    {{text}}
                  </button>
                {{/with}}
              </div>
            {{/with}}
          </div>
          <h6>{{header_text}}</h6>
          {{#with button}}
            <span>
              <button
                type='button'
                class='btn btn-info'
              >{{text}}
              </button>
            </span>
          {{/with}}
        {{/with}}
      </div>
    </div>
  </section>
  <section>
    <div class='card'>
      <div class='card-body'>
        {{#with paragraph}}
          <p class='text-uppercase'>{{title}}</p>
          <p class='text-lowercase'>{{text}} </p>
          {{#with button}}
            <a href='{{url}}' class='card-link'>{{text}}</a>
          {{/with}}
          <br />
          <br />
          <div class='card'>
            <div class='card-body'>
              {{#each list_name}}
                <ul>
                  <li>
                    <a
                      href='{{url}}'
                      class='card-link'
                    >{{text}}</a>
                  </li>
                </ul>
              {{/each}}
            </div>
          </div>
        {{/with}}
      </div>
    </div>
  </section>
</div>