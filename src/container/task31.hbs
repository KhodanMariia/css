<main>
  <section>
    {{#with navigation}}
      <table class='table'>
        <theder>
          <tr>
            {{#each links}}
              <th>
                <a href='{{href}}'>{{text}}</a>
              </th>
            {{/each}}
            {{#with button}}
              <th>
                <a
                  class='btn btn-primary btn-sm'
                  href='{{href}}'
                  role='button'
                >{{text}}
                </a>
              </th>
            {{/with}}
          </tr>
        </theder>
      </table>
    {{/with}}
  </section>
  <section>
    <br />
    <div>
      {{#with header}}
        <h1>{{title}}</h1>
        <br />
        <p>{{description}}</p>
        {{#with button}}
          <a
            class='btn btn-primary btn-sm'
            href='{{href}}'
            role='button'
          >{{text}}
          </a>
        {{/with}}
      {{/with}}
    </div>
    <br />
  </section>
  <hr />
  <section>
    <div>
      <br />
      {{#with city}}
        <h2>{{title}}</h2>
        <br />
        <div class='row g-4'>
          {{#each cards}}
            <div class='col-6'>
              <div class='card'>
                {{#with picture}}
                  <img
                    class='card-img-top'
                    src='{{src}}'
                    alt='{{alt}}'
                  />
                {{/with}}
                <div class='card-body'>
                  <h6 class='card-title'>{{title}}</h6>
                  <p>{{description}}</p>
                  {{#with button}}
                    <a
                      class='btn btn-primary'
                      href='{{href}}'
                      role='button'
                    >{{text}}</a>
                  {{/with}}
                </div>
              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      <br />
      {{#with winner}}
        <h2>{{title}}</h2>
        <br />
        <table class='table'>
          {{#with table}}
            <thead>
              {{#with head}}
                <tr>
                  <th>
                    {{0}}
                  </th>
                  <th>
                    {{1}}
                  </th>
                  <th>
                    {{2}}
                  </th>
                  <th>
                    {{3}}
                  </th>
                </tr>
              {{/with}}
            </thead>
            <tbody>
              {{#each user}}
                <tr>
                  <th>
                    {{place}}
                  </th>
                  <th>
                    {{name}}
                  </th>
                  <th>
                    {{surname}}
                  </th>
                  <th>
                    {{nickname}}
                  </th>
                </tr>
              {{/each}}
            </tbody>
          {{/with}}
        </table>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      <br />
      {{#with gallery}}
        <div class='display-3'>
          {{title}}
        </div>
        <br />
        <div class='row'>
          {{#each cards}}
            <div class='card col-4'>
              <p class='card-header'>
                {{name}}
              </p>
              <ul class='list-group'>
                {{#with items}}
                  <li class='list-group-item'>{{0}}</li>
                  <li class='list-group-item'>{{1}}</li>
                  <li class='list-group-item'>{{2}}</li>
                {{/with}}
              </ul>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      {{#with relaxion}}
        <div class='display-2'>{{title}}</div>
        <br />
        <div class='row g-4'>
          {{#each cards}}
          <div class="col-5">
            <div class='card text-bg-secondary'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
              </div>
            </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      {{#with rest}}
        <br />
        <div class='row gx-4'>
          {{#each cards}}
            <div class='card text-center col-6'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
              </div>
              <div class='card-footer'>{{footer}}</div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      {{#with proposition}}
        <div class='display-2'>{{title}}</div>
        <br />
        <div class='row g-4'>
          {{#each card}}
            <div class='card col-6 text-bg-warning'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
  <hr />
  <section>
    <div>
      {{#with footer}}
        <h5>{{title}}</h5>
        <div class='row g-4'>
            <div class='card col-3'>
              <div class='card-body'>
                {{#each navigation}}
                  <div>
                    <a href='{{href}}'>{{text}}</a>
                  </div>
                  <br>
                {{/each}}
              </div>
            </div>
             <div class='card col-3'>
              <div class='card-body'>
                {{#each socialMedia}}
                  <div>
                    <a class='btn  btn-outline-secondary btn-sm' href='{{link}}'>{{name}}</a>
                  </div>
                  <br>
                {{/each}}
              </div>
            </div>
             <div class='card col-3'>
              <div class='card-body'>
                {{#each partners}}
                  <div>
                    <a class='btn btn-outline-info btn-sm' href='{{link}}'>{{name}}</a>
                  </div>
                  <br>
                {{/each}}
              </div>
            </div>
          </div>
        </div>
      {{/with}}
    </div>
  </section>
</main>